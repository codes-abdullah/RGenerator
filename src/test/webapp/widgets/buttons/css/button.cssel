@charset "UTF-8";



.${R.html_classes.BUTTON_CONTAINER}{
	--button-min-width: 120px;
	--button-min-height: 30px;	
	--button-background-color: #FCFCFC;
	--button-background-hover-color: #f5f5f5;
	--button-background-click-color: white;
	--button-border-color: #DED7FF;
	--button-border-hover-color: #A799E7;
	--button-border-radius: 10px;	
	--button-font-color: #1F1485;
	--button-font-hover-color: var(--button-font-color);
	--button-font-click-color: var(--button-font-color);
	--button-font-size: 14px;
	
	min-width: var(--button-min-width);
	min-height: var(--button-min-height);
	border: 1px solid var(--button-border-color);
	border-radius:var(--button-border-radius);  
	align-content: center;
	align-items: center;
	align-self: center;
	text-align: center;
}

.${R.html_classes.BUTTON}{	
  	text-decoration: none;
  	background: var(--button-background-color);
	border-radius: var(--button-border-radius);
	color: var(--button-font-color);
	font-size: var(--button-font-size);	
}

.${R.html_classes.BUTTON_CONTAINER}:hover {
	background: var(--button-background-hover-color);
	cursor: pointer;
	color: var(--button-font-hover-color);
	outline: 1px solid var(--button-border-hover-color);
	
}

.${R.html_classes.BUTTON_CONTAINER}:hover .${R.html_classes.BUTTON_CONTAINER}{
	
}


.${R.html_classes.BUTTON_CONTAINER}:active{
	background: var(--button-background-click-color);
	color: var(--button-font-click-color);
}

.${R.html_classes.BUTTON_CONTAINER} A{
	display: block;
	width: 100%;
	height: 100%;
	
}